#!/bin/bash
date -s "$(wget -qSO- --max-redirect=0 baidu.com 2>&1 | grep Date: | cut -d' ' -f5-8)Z"
hwclock -w

/usr/local/bin/ui-saveListBW

ipset -F listB
for iplistB in $(cat /usr/local/bin/IPlistB); do
  ipset add listB $iplistB
done

ipset -F listW
for iplistW in $(cat /usr/local/bin/IPlistW); do
  ipset add listW $iplistW
done